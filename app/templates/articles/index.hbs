<label>
  {{input type="checkbox" checked=showReturned}} <span class="checkable">Show returned items</span>
</label>
<table class="primary">
	<thead>
		<tr>
			<th>Description</th>
			<th>Notes</th>
			<th>Borrowed Since</th>
			<th colspan="2">Status</th>
		</tr>
	</thead>
	<tbody>
		{{#each filteredResults as |article|}}
			{{articles/article-row  article=article save="save" articleStates=possibleStates}}
		{{/each}}
	</tbody>
</table>